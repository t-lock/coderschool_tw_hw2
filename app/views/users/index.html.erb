<br>
<h1>Find Friends</h1>

<div class="panel panel-default">
  <div class="panel-heading">Users</div>
  <table class="table">
    <thead>
      <tr>
        <th>Username</th>
        <th>Friendship</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= link_to 'Add Friend', '#', class: 'btn btn-primary' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

